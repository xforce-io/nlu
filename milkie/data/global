PATTERN_EXPR_UNION_BEGIN(whatis)

  whatIsPhrase = [$WhatIs, $HowToInterpret]
  whatissocalled = { {$SoCalled ?} $*sbv {$Def ?} }
  { $Greeting { $YouKnow ?} $whatIsPhrase $whatissocalled $ModalParticleWithPunc }

  isWhatPhrase = [$IsWhat, $HowToInterpret]
  { $Greeting { $YouKnow ?} {$SoCalled ?} $*sbv $Def {$isWhatPhrase ?} $ModalParticleWithPunc }
  { $Greeting { $YouKnow ?} {$SoCalled ?} $*sbv {$Def ?} $isWhatPhrase $ModalParticleWithPunc }

  <sbv>
PATTERN_EXPR_UNION_END

